{
  "language": "zh-hk",
  "intents": {
    "HassTurnOff": {
      "data": [
        {
          "sentences": [
            "<turn_off>[<area>]{name}",
            "[<let>][<area>]{name}<turn_off>"
          ],
          "excludes_context": {
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "<turn_off> <area> <light>",
            "[<let>] <area> <light> <turn_off>"
          ],
          "slots": {
            "domain": "light"
          },
          "response": "lights_area"
        },
        {
          "sentences": [
            "<close>[<area>]{name}",
            "[<let>][<area>]{name}<close>"
          ],
          "requires_context": {
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "<close>車房[的][大|捲閘]門",
            "[<let>]車房[的][大|捲閘]門<close>"
          ],
          "slots": {
            "domain": "cover",
            "device_class": "garage"
          }
        },
        {
          "sentences": [
            "<close>[<area>]{cover_classes:device_class}",
            "[<let>][<area>]{cover_classes:device_class}<close>"
          ],
          "slots": {
            "domain": "cover"
          },
          "response": "cover_device_class"
        },
        {
          "sentences": [
            "<turn_off> <area> (風扇 | 吊扇)"
          ],
          "slots": {
            "domain": "fan",
            "name": "all"
          },
          "response": "fans_area"
        }
      ]
    },
    "HassTurnOn": {
      "data": [
        {
          "sentences": [
            "<turn_on> <area> <light>",
            "[<let>] <area> <light> <turn_on>"
          ],
          "slots": {
            "domain": "light"
          },
          "response": "lights_area"
        },
        {
          "sentences": [
            "<turn_on><area>(風扇|吊扇)"
          ],
          "slots": {
            "domain": "fan",
            "name": "all"
          },
          "response": "fans_area"
        },
        {
          "sentences": [
            "<turn_on>[<area>]{name}",
            "[<let>][<area>]{name}<turn_on>"
          ],
          "excludes_context": {
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "<open>[<area>]{name}",
            "[<let>][<area>]{name}<open>"
          ],
          "requires_context": {
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "<open>車房[的][大|捲閘]門",
            "[<let>]車房[的][大|捲閘]門<open>"
          ],
          "slots": {
            "domain": "cover",
            "device_class": "garage"
          }
        },
        {
          "sentences": [
            "<open>[<area>]{cover_classes:device_class}",
            "[<let>][<area>]{cover_classes:device_class}<open>"
          ],
          "slots": {
            "domain": "cover"
          },
          "response": "cover_device_class"
        }
      ]
    },
    "HassLightSet": {
      "data": [
        {
          "sentences": [
            "(設置|調節|調整|調教)<name>(亮度)[<set_to>]<brightness>"
          ],
          "response": "brightness",
          "requires_context": {
            "domain": "light"
          }
        },
        {
          "sentences": [
            "(設置|調節|調整|調教)<area>[<light>](亮度)[<set_to>]<brightness>"
          ],
          "slots": {
            "name": "all"
          },
          "response": "brightness_area"
        },
        {
          "sentences": [
            "(設置|調節|調整|調教)<name>[<light>](亮度)[<set_to>]{brightness_level:brightness}"
          ],
          "requires_context": {
            "domain": "light"
          },
          "response": "brightness"
        },
        {
          "sentences": [
            "(設置|調節|調整|調教)<area>[<light>](亮度)[<set_to>]{brightness_level:brightness}"
          ],
          "slots": {
            "name": "all"
          },
          "response": "brightness_area"
        },
        {
          "sentences": [
            "(設置|調節|調整|調教)<name>[顏色][<set_to>]{color}"
          ],
          "requires_context": {
            "domain": "light"
          },
          "response": "color"
        },
        {
          "sentences": [
            "(設置|調節|調整|調教)<area><light>[顏色][<set_to>]{color}"
          ],
          "slots": {
            "name": "all"
          },
          "response": "color_area"
        }
      ]
    },
    "HassClimateGetTemperature": {
      "data": [
        {
          "sentences": [
            "[<area>] <temp> <how_many_is>",
            "[<area>](現在|依家)多少度",
            "[<area>](現在|依家)幾度"
          ]
        }
      ]
    },
    "HassGetState": {
      "data": [
        {
          "sentences": [
            "[<area>]<name>[狀態](<what_is>|<how_many_is>)",
            "[<area>]<name>[<what_is>]狀態"
          ],
          "response": "one"
        },
        {
          "sentences": [
            "[<area>]{name}係唔係{on_off_states:state}",
            "[<area>]{name}[是]{on_off_states:state}[嗎|咗|咗嗎]"
          ],
          "response": "one_yesno",
          "excludes_context": {
            "domain": [
              "cover"
            ]
          }
        },
        {
          "sentences": [
            "[{area}][有|有没有|有冇|有無]{on_off_domains:domain}[是|是不是|係唔係]{on_off_states:state}[嗎|不|咗嗎]"
          ],
          "response": "any"
        },
        {
          "sentences": [
            "[<area>][<all>]{on_off_domains:domain}[是|是不是|係唔係]都[是|係]{on_off_states:state}[嗎|不]"
          ],
          "response": "all"
        },
        {
          "sentences": [
            "[<area>]<which>{on_off_domains:domain}[是|係]{on_off_states:state}",
            "<which>[<area>]{on_off_domains:domain}[是]{on_off_states:state}"
          ],
          "response": "which"
        },
        {
          "sentences": [
            "[{area}]<how_many_is>{on_off_domains:domain}[是|係]{on_off_states:state}",
            "<how_many_is>[<area>]{on_off_domains:domain}[是|係]{on_off_states:state}"
          ],
          "response": "how_many"
        },
        {
          "sentences": [
            "[<area>]{name}(是|是不是|係|係唔係){cover_states:state}[嗎|不]"
          ],
          "response": "one_yesno",
          "requires_context": {
            "domain": "cover"
          },
          "slots": {
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "[{area}][有|有没有|有無]{cover_classes:device_class}[是|是不是|係|係唔係]{cover_states:state}[嗎|不]"
          ],
          "response": "any",
          "slots": {
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "[<area>][<all>]{cover_classes:device_class}[是|是不是|係|係唔係]都[是|係]{cover_states:state}[嗎|不]",
            "<all><area>{cover_classes:device_class}[是|是不是|係|係唔係]都[是|係]{cover_states:state}[嗎|不]"
          ],
          "response": "all",
          "slots": {
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "[<area>]<which>{cover_classes:device_class}[是|係]{cover_states:state}",
            "<which>[<area>]{cover_classes:device_class}[是|係]{cover_states:state}"
          ],
          "response": "which",
          "slots": {
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "[{area}]<how_many_is>{cover_classes:device_class}[是|係]{cover_states:state}",
            "<how_many_is>[<area>]{cover_classes:device_class}[是|係]{cover_states:state}"
          ],
          "response": "how_many",
          "slots": {
            "domain": "cover"
          }
        }
      ]
    }
  },
  "settings": {
    "ignore_whitespace": true
  },
  "responses": {
    "errors": {
      "no_intent": "對唔住，我聽不明白",
      "no_area": "沒有區域名叫 {{ area }}",
      "no_domain_in_area": "區域名 {{ area }} 不包含 {{ domain }}",
      "no_device_class_in_area": "區域名 {{ area }} 不包含 {{ device_class }}",
      "no_entity": "無設備或實體叫 {{ entity }}",
      "handle_error": "處理時發生意外錯誤"
    },
    "intents": {
      "HassGetState": {
        "one": "{{ slots.name }}現在係{{state.state_with_unit}}",
        "one_yesno": "{% if query.matched: %}\n係呀\n{% else: %}\n唔係呀，{{ slots.name }}係{{ state.state }}咗\n{% endif %}\n",
        "any": "{% if query.matched %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  係呀，{{ match[:4] | join(\"、\") }}{{ '等' if match|length > 4 else '' }}\n  {{- slots.state[:-1] if slots.state[-1] == '的' else slots.state }}\n{%- else -%}\n  無呀\n{% endif %}\n",
        "all": "{% if not query.unmatched: %}\n  係呀\n{%- else -%}\n  {% set no_match = query.unmatched | map(attribute=\"name\") | sort | list %}\n  唔係呀，{{ no_match[:4] | join(\"、\") }}{{ '等' if match|length > 4 else '' }}除外\n{% endif %}\n",
        "which": "{% if not query.matched %}\n  無呀\n{% else: %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  {{ match[:4] | join(\"、\") }}{{ '等' if match|length > 4 else '' }}{{ slots.state }}\n{% endif %}\n",
        "how_many": "{{ query.matched | length }}\n"
      },
      "HassTurnOff": {
        "default": "{{ slots.name|default('') }}已關閉",
        "lights_area": "{{ slots.area }}燈已關閉",
        "fans_area": "{{ slots.area }}風扇已關閉",
        "cover": "{{ slots.name }}已關閉",
        "cover_device_class": "{{ slots.area|default('') }}{{ slots.device_class }}已關閉",
        "cover_area": "{{ slots.area }}窗簾已關閉",
        "cover_door": "{{ slots.area }}門已關閉",
        "cover_window": "{{ slots.area }}窗戶已關閉",
        "lock": "{{ slots.name|default('') }}已解鎖"
      },
      "HassTurnOn": {
        "default": "{{ slots.name }}已打開",
        "lights_area": "{{ slots.area }}燈已打開",
        "fans_area": "{{ slots.area }}風扇已打開",
        "cover": "{{ slots.name }}已打開",
        "cover_device_class": "{{ slots.area|default('') }}{{ slots.device_class }}已打開",
        "cover_area": "{{ slots.area }}窗簾已打開",
        "cover_door": "{{ slots.area }}門已打開",
        "cover_window": "{{ slots.area }}窗戶已打開",
        "scene": "{{ slots.name|default('') }}已開啟",
        "script": "{{ slots.name|default('') }}腳本已執行",
        "lock": "{{ slots.name|default('') }}已上鎖"
      },
      "HassLightSet": {
        "brightness": "{{ slots.name }}亮度已設置為{{ slots.brightness }}",
        "brightness_area": "{{ slots.area }}燈亮度已設置為{{ slots.brightness }}",
        "color": "{{ slots.name }}顏色已設置為{{ slots.color }}",
        "color_area": "{{ slots.area }}燈顏色已設置為{{ slots.color }}"
      },
      "HassClimateGetTemperature": {
        "default": "{{ state.state }} 度"
      }
    }
  },
  "lists": {
    "color": {
      "values": [
        {
          "in": "红色",
          "out": "red"
        },
        {
          "in": "橙色",
          "out": "orange"
        },
        {
          "in": "黄色",
          "out": "yellow"
        },
        {
          "in": "綠色",
          "out": "green"
        },
        {
          "in": "藍色",
          "out": "blue"
        },
        {
          "in": "紫色",
          "out": "purple"
        },
        {
          "in": "白色",
          "out": "white"
        },
        {
          "in": "黑色",
          "out": "black"
        },
        {
          "in": "棕色",
          "out": "brown"
        }
      ]
    },
    "brightness": {
      "range": {
        "type": "percentage",
        "from": 0,
        "to": 100
      }
    },
    "temperature": {
      "range": {
        "type": "temperature",
        "from": 0,
        "to": 100
      }
    },
    "temperature_unit": {
      "values": [
        {
          "in": "c|攝氏|度|°C|℃",
          "out": "celsius"
        },
        {
          "in": "f|華氏|度|°F|℉",
          "out": "fahrenheit"
        }
      ]
    },
    "brightness_level": {
      "values": [
        {
          "in": "(最大|最光|最高|最亮)",
          "out": 100
        },
        {
          "in": "(最小|最暗|最低|最黑)",
          "out": 1
        }
      ]
    },
    "on_off_states": {
      "values": [
        {
          "in": "(打開|開著|開了|開咗|開)[的]",
          "out": "on"
        },
        {
          "in": "(關閉|關著|關了|關咗|閂咗|閂)[的]",
          "out": "off"
        }
      ]
    },
    "on_off_domains": {
      "values": [
        {
          "in": "(燈|天花燈|筒燈|射燈|檯燈|床頭燈|燈光)",
          "out": "light"
        },
        {
          "in": "(風扇|電風扇|塔扇|吊扇)",
          "out": "fan"
        },
        {
          "in": "(開關|開關制|電源|插座|通斷器)",
          "out": "switch"
        }
      ]
    },
    "cover_states": {
      "values": [
        {
          "in": "(打開|開著|開了|開咗|打開)[的|咗]",
          "out": "open"
        },
        {
          "in": "(關閉|關著|關了|關咗|閂咗)[的|咗]",
          "out": "closed"
        },
        {
          "in": "打開緊",
          "out": "opening"
        },
        {
          "in": "關閉緊",
          "out": "closing"
        }
      ]
    },
    "cover_classes": {
      "values": [
        {
          "in": "(遮陽|雨)(蓬|棚)",
          "out": "awning"
        },
        {
          "in": "(百葉窗|百葉簾|捲簾)",
          "out": "blind"
        },
        {
          "in": "窗簾",
          "out": "curtain"
        },
        {
          "in": "(門|房門|捲閘門)",
          "out": "door"
        },
        {
          "in": "車房門",
          "out": "garage"
        },
        {
          "in": "(大門|院子門|鐵門)",
          "out": "gate"
        },
        {
          "in": "遮陽簾",
          "out": "shade"
        },
        {
          "in": "百葉窗戶",
          "out": "shutter"
        },
        {
          "in": "窗戶",
          "out": "window"
        }
      ]
    },
    "lock_states": {
      "values": [
        {
          "in": "(已上鎖|鎖著|鎖上|鎖了|鎖咗)[的|嘅]",
          "out": "locked"
        },
        {
          "in": "(已解鎖|沒[有][上]鎖|無[上]鎖)[的|嘅]",
          "out": "unlocked"
        }
      ]
    }
  },
  "expansion_rules": {
    "name": "{name}[的|嘅]",
    "area": "{area}[的|嘅]",
    "let": "[請|把|给|讓]",
    "all": "(全部|所有)[的|嘅]",
    "what_is": "是什麼",
    "how_many_is": "(是|有)(多少|幾|幾個)",
    "which": "[有|有没有|有無]哪[一](個|些)",
    "brightness": "[百分之]{brightness}[%]",
    "to": "(為|到|至)",
    "set_to": "(設置|設定|調節|調)(為|到|至)",
    "turn_on": "(打開|開啟|開|着)",
    "turn_off": "(關閉|關掉|關了|關|閂)",
    "open": "(打開|開)",
    "close": "(關閉|關掉|關了|關|閂)",
    "light": "(燈|吸頂燈|筒燈|射燈|檯燈|台燈|床頭燈|燈光)[的|嘅]",
    "temp": "溫度",
    "temperature": "{temperature} [{temperature_unit}]"
  },
  "skip_words": []
}